# Environment files in the /etc/profile.d dir.
profiles::base::environment::env_hash:
  "10-environments.sh": |
    # Managed by Puppet
    export HOSTGROUP="%{::hostgroup}"
    [ -z $KSSUSER ] && KSSUSER="UNKNOWN"
    BASHPRE=${HOSTGROUP^^}:${KSSUSER}
    export PS1="[\${BASHPRE}:\u@\H \w]\\$ "
    export PATHPRE=/usr/local/cdnet/scripts
    export PATH=$PATHPRE:$PATH:$PATHPOST
    [ -z $TMOUT ] && readonly TMOUT=900
  "20-kernel.sh": |
    # Managed by Puppet
    export DAEMON_COREFILE_LIMIT=unlimited

# Default SSH options
ssh::server_options:
  AcceptEnv: ENC[PKCS7,]
ssh::client_options:
    'Host *':
        SendEnv: ENC[PKCS7,]
        ForwardX11Trusted: 'yes'

# Default login allowed users
profiles::base::pam::allowed_users:
  - ENC[PKCS7,]
  - ENC[PKCS7,]
  - ENC[PKCS7,]
  - ENC[PKCS7,]

# Default email aliase settings
profiles::base::aliases::aliases:
  'deploy':
    recipient: ENC[PKCS7,]
  'ladmin':
    recipient: ENC[PKCS7,]
